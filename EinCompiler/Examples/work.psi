include("das-os.psi");

var scalar : int;
var array  : int[10];

fn main() -> int
	var i : int;
{
	scalar := 'A';
	putc(scalar);
	puti(scalar);

	scalar := 1;

	array[0]      := 10;
	array[scalar] := 20;

	puti(arrayToInt(array));
	puti(array[0]);
	puti(array[scalar]);
	putc('B');
}

naked inline fn arrayToInt(a : int[10]) -> int [[ ]]

